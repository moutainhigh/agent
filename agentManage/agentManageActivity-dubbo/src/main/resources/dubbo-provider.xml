<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


   	<!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="pms-dubbo-server"/>

   <dubbo:registry protocol="zookeeper" address="${zk.server}" group="${zk.group}" id="myGroup"/>
    <!-- 用dubbo协议在22880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="${zk.port}"/>
    <dubbo:annotation package="com.ryx.credit.service.*" />
    <dubbo:service interface="com.ryx.credit.service.ActivityService" ref="activityService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ActHiVarinstService" ref="actHiVarinstService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ActIdUserService" ref="actIdUserService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ActRuTaskService" ref="actRuTaskService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ActUtilService" ref="actUtilService"  timeout="1200000" registry="${zk.group}"/>

    <dubbo:reference interface="com.ryx.credit.service.agent.AgentEnterService" id="agentEnterService"   timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.agent.DataChangeActivityService" id="dataChangeActivityService"   timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.order.OSupplementService" id="oSupplementService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.order.OrderService" id="orderService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.StagingService" id="stagingServiceImpl"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.ProfitMonthService" id="profitMonthService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.ToolsDeductService" id="toolsDeductService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.order.CompensateService" id="compensateService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.order.IOrderReturnService" id="orderReturnService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IPosRewardService" id="iPosRewardService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IPosCheckService" id="iPosCheckService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IPTaxAdjustService" id="iPTaxAdjustService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.IUserService" id="iUserService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.agent.BusActRelService" id="busActRelService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.agent.ApprovalFlowRecordService" id="approvalFlowRecordService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IProfitAgentMergerService" id="profitAgentMergerService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:reference interface="com.ryx.credit.service.order.TerminalTransferService" id="terminalTransferService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.agent.AgentMergeService" id="agentMergeService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IAgentRelateService" id="agentRelateService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.profit.service.IProfitCityApplicationService" id="profitCityApplicationService"  timeout="1200000" registry="${zk.group}" check="false"/>

</beans>