<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    

   	<!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="pms-dubbo-server"/>
 
   <dubbo:registry protocol="zookeeper" address="${zk.server}" group="${zk.group}" id="myGroup"/>
    <!-- 用dubbo协议在22880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="${zk.port}"/>
    <dubbo:annotation package="com.ryx.credit.service.*" />
    <dubbo:service interface="com.ryx.credit.service.IOrganizationService" ref="organizationService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.IResourceService" ref="resourceService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.IRoleResourceService" ref="roleResourceService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.IRoleService" ref="roleService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ISysLogService" ref="sysLogService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.IUserRoleService" ref="userRoleService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.IUserService" ref="userService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICConditionService" ref="cConditionService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICConditionValueService" ref="cConditionValueService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICRuleConditionService" ref="cRuleConditionService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICRulePlatformService" ref="cRulePlatformService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICRuleService" ref="cRuleService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICStrategyRuleService" ref="cStrategyRuleService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.ICStrategyService" ref="cStrategyService"  timeout="1200000" registry="${zk.group}"/>

    <dubbo:service interface="com.ryx.credit.service.dict.DictOptionsService" ref="dictOptionsService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.BusinessPlatformService" ref="businessPlatformService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.dict.RegionService" ref="regionService" timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentContractService" ref="agentContractService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentService" ref="agentService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentBusinfoService" ref="agentBusinfoService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AccountPaidItemService" ref="accountPaidItemService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.BusinessCAService" ref="businessCAService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentQueryService" ref="agentQueryService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.dict.MultiFileUploadService" ref="multiFileUploadService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentEnterService" ref="agentEnterService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.dict.DepartmentService" ref="departmentService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.ApaycompService" ref="apaycompService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.TaskApprovalService" ref="taskApprovalService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.DateChangeReqService" ref="dateChangeReqService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentAssProtocolService" ref="agentAssProtocolService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.DataChangeActivityService" ref="dataChangeActivityService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AimportService" ref="aimportService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.AgentNotifyService" ref="agentNotifyService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.agent.BusActRelService" ref="busActRelService"  timeout="1200000" registry="${zk.group}"/>


    <dubbo:reference interface="com.ryx.credit.service.ActivityService" id="activityService"  timeout="1200000" registry="${zk.group}" check="false"/>
    <dubbo:reference interface="com.ryx.credit.service.ActUtilService" id="actUtilService"  timeout="1200000" registry="${zk.group}" check="false" />

    <!--订单系统-->
    <dubbo:service interface="com.ryx.credit.service.order.ProductService" ref="productService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.order.OrderService" ref="orderService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.order.AddressService" ref="addressService"  timeout="1200000" registry="${zk.group}"/>
    <dubbo:service interface="com.ryx.credit.service.order.OrderActivityService" ref="orderActivityService"  timeout="1200000" registry="${zk.group}"/>

</beans>